<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tech.sparrow.gretel.UserActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="206dp"
        android:layout_height="185dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="52dp"
        app:srcCompat="@drawable/atomix_user31"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="59dp" />

    <Button
        android:id="@+id/userMarks_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="63dp"
        android:text="Owened Marks"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="299dp" />

    <ProgressBar
        android:id="@+id/scanningBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/userMarks_btn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="412dp" />

</android.widget.RelativeLayout>
